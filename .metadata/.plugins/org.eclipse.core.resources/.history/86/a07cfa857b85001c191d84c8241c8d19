<%
	String USA = request.getParameter("spot");
	String Japan = request.getParameter("spot");
	String Austrailia = request.getParameter("spot");
	String Canada = request.getParameter("spot");
	
	if(USA.equals("USA")){
		response.sendRedirect("travel_USA.jsp");
	}else if(Japan.equals("Japan")){
		response.sendRedirect("travel_Japan.jsp");
	}else if(Austrailia.equals("Austrailia")){
		response.sendRedirect("travel_Austrailia.jsp");
	}else{
		response.sendRedirect("travel_Canada.jsp");
	}
%>
